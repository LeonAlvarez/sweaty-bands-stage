<layout>

    <!-- handles new tab  product edit option for  offline sync-->   
    <add_i95dev_product_tab>
        <reference name="product_tabs">
            <action method="addTab">
                <name>offlinesynctab</name>
                <block>I95DevConnect_Platinum/GP_adminhtml_catalog_product_tab</block>
            </action>
        </reference>
    </add_i95dev_product_tab>
    
    <!-- supports simple product -->   
    <adminhtml_catalog_product_edit>
        <update handle="add_i95dev_product_tab"/>
    </adminhtml_catalog_product_edit>
   
    
    
    

</layout>