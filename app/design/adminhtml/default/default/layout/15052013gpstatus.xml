<layout version="0.1.0">
 
    <adminhtml_status_index>
        <remove name="header"/>
        <remove name="menu"/>
        <remove name="notifications"/>
        <remove name="footer" />
        <reference name="head">
            <action method="addCss">
                <stylesheet>i95dev.css</stylesheet>
            </action>
        </reference>
        <reference name="content">
            <block type="gpstatus/adminhtml_status" name="gpstatus"></block>
        </reference>
    </adminhtml_status_index>

</layout> 