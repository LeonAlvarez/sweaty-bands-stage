        <Magestore_Customtabs>
            <version>0.1.0</version>
        </Magestore_Customtabs>
    </modules>
    <global>
        <blocks>
            <customtabs>
                <class>Magestore_Customtabs_Block</class>
            </customtabs>
        </blocks>
